<p id="notice"><%= notice %></p>

<h1>Directorios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Telefono</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @directorios.each do |directorio| %>
      <tr>
        <td><%= directorio.nombre %></td>
        <td><%= directorio.apellido %></td>
        <td><%= directorio.telefono %></td>
        <td><%= link_to 'Show', directorio %></td>
        <td><%= link_to 'Edit', edit_directorio_path(directorio) %></td>
        <td><%= link_to 'Destroy', directorio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Directorio', new_directorio_path %>
